[run]
source = src
omit =
    */site-packages/*
    */tests/*
    */__init__.py
    */tmp/*
    */_remote_module_non_scriptable.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == "__main__":
    pass
    raise ImportError
    except ImportError
    if TYPE_CHECKING:

[html]
directory = coverage_html 